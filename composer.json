{
  "name": "bentools/meilisearch-odm",
  "description": "Meilisearch Object Document Mapper",
  "type": "library",
  "require": {
    "php": "^8.4",
    "bentools/iterable-functions": "^2.3",
    "bentools/meilisearch-filters": "^1.0",
    "bentools/set": "^1.1",
    "meilisearch/meilisearch-php": "~1.13",
    "symfony/options-resolver": "~5.0|~6.0|~7.0",
    "symfony/property-access": "~5.0|~6.0|~7.0",
    "symfony/serializer": "~5.0|~6.0|~7.0"
  },
  "require-dev": {
    "nyholm/psr7": "^1.8",
    "pestphp/pest": "^3.7",
    "phpstan/phpstan": "^2.1",
    "psr/http-factory": "^1.1",
    "symfony/http-client": "^7.2",
    "symfony/var-dumper": "^7.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BenTools\\MeilisearchOdm\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\MeilisearchOdm\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Benoit POLASZEK"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  }
}
